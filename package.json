{
  "name": "workflow",
  "version": "1.0.0",
  "description": "Workflow onboarding tracker",
  "author": {
    "name": "David Citarelli",
    "email": "davidcit646@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "./scripts/run-tauri.sh dev",
    "dev": "./scripts/run-tauri.sh dev",
    "dev:release": "./scripts/run-tauri.sh dev --release",
    "dist": "./scripts/run-tauri.sh build",
    "dist:linux": "./scripts/run-tauri.sh build --bundles appimage",
    "dist:win": "./scripts/run-tauri.sh build --bundles nsis",
    "tauri": "./scripts/run-tauri.sh",
    "lint": "eslint \"web/**/*.js\"",
    "format": "prettier --write \"web/**/*.js\" \"web/**/*.css\" \"web/**/*.html\"",
    "audit:runtime": "npm audit --omit=dev",
    "audit:fix:safe": "npm audit fix --omit=dev",
    "audit:all": "npm audit"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {},
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5"
  },
  "overrides": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0"
  }
}
